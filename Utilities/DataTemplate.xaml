<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ProductsClient.ViewModel"
                    xmlns:view="clr-namespace:ProductsClient.Views">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductsVM}">
        <view:ProductsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AuthentiactionVM}">
        <view:AuthenticationView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SellProductsVM}">
        <view:SellProductsView/>
    </DataTemplate>
</ResourceDictionary>